{
	"compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "outDir": "dist",
        "strictNullChecks": true,
        "sourceMap": true,
        "target": "ES5",
        "typeRoots": [
          "node_modules/@types"
        ]
        
    },
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
        "dist",
        "**/*.spec.ts"
	]
}