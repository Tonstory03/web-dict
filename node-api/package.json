{
  "name": "node-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "dev": "nodemon --watch  src/**/*.ts --ignore src/**/*.spec.ts --exec ts-node src/index.ts",
    "prepare": "node ./scripts/hook",
    "debug": "nodemon --watch  test.ts --ignore src/**/*.spec.ts --exec ts-node ./test.ts",
    "start:dev": "NODE_ENV=dev node ./dist/index.js",
    "start:test": "NODE_ENV=test node ./dist/index.js",
    "start:staging": "NODE_ENV=test node ./dist/index.js",
    "start:prod": "NODE_ENV=production node ./dist/index.js",
    "start": "node ./dist/index.js",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "mongoose": "^5.12.14",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.10.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/morgan": "^1.9.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
